{% extends "layouts/base.njk" %}
{% set pageCriticalStyles = ['critical/docs-critical.css'] %}
{% set pageScripts = ['https://cdnjs.cloudflare.com/ajax/libs/prism/1.22.0/prism.min.js'] %}
{% set pageStylesheets = ['../../css/docs.css', 'https://cdnjs.cloudflare.com/ajax/libs/prism/1.22.0/themes/prism-okaidia.min.css'] %}

{% block content %}
<div class="container docs">
    <button class="docs__toggle-nav">Navigate Docs</button>
    <aside class="docs__navigation">docs links go here</aside>
    <article class="docs__inner">{{content | safe}}</article>
</div>
{% endblock %}